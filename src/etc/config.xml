<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <nanobots_datapatchcreator>
            <settings>
                <enabled>1</enabled>
                <module>Nanobots_DataPatchCreator</module>
            </settings>
            <export>
                <timestamp>1</timestamp>
                <namespace>Nanobots</namespace>
                <module>DataPatchCreator</module>
                <type>0</type>
                <local_path>{{baseDir}}/var/patches/</local_path>
            </export>
            <rules>
                <behavior>0</behavior>
                <active>1</active>
            </rules>
            <images>
                <swatches>1</swatches>
                <sync>1</sync>
                <sync_model>LocalFile</sync_model>
                <local_folder>{{baseDir}}/var/images/</local_folder>
            </images>
        </nanobots_datapatchcreator>
    </default>
</config>
